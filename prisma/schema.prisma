generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Testimonial{
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  email       String
  message     String
  isPublished Boolean
  createdAt   DateTime  @default(now()) 
} 

